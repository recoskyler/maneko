<% if (Object.prototype.toString.call(error) === "[object String]") { %>
    <%- include("./message.ejs", { type: "error", message: error }) %>
<% } else if (Object.prototype.toString.call(error) === "[object Array]") { %>
    <% error.forEach(msg => { %>
        <%- include("./message.ejs", { type: "error", message: msg }) %>
    <% }) %>
<% } %>

